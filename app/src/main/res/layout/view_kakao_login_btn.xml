<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/btn_kakao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/custom_login_button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:background="@drawable/kakao_symbol"
            app:layout_constraintBottom_toBottomOf="@id/btn_kakao"
            app:layout_constraintEnd_toEndOf="@id/btn_kakao"
            app:layout_constraintStart_toStartOf="@id/btn_kakao"
            app:layout_constraintTop_toTopOf="@id/btn_kakao" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
